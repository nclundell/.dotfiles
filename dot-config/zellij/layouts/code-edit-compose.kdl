layout {
  default_tab_template {
    pane size=1 borderless=true {
      plugin location="compact-bar"
    }
    children
  }

  tab name="Editor" {
    pane {
      command "nvim"
    }
  }

  tab name="Docker" {
    pane {
      command "docker"
      args "compose" "up" "--remove-orphans"
    }
  }
}
